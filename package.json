{
  "name": "nestjs-oraq",
  "version": "0.2.5",
  "description": "Ordered redis asynchronous queue",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "rm -rf dist && yarn tsc -p tsconfig.build.json",
    "test": "NODE_ENV=test node ./test/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bardak-dev/nestjs-redis"
  },
  "keywords": [
    "concurrency",
    "queue",
    "limiter",
    "pool",
    "redis",
    "job",
    "task"
  ],
  "author": "Yevhen Samoilenko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deugene/oraq/issues"
  },
  "homepage": "https://github.com/deugene/oraq#readme",
  "devDependencies": {
    "@nestjs/common": "7.6.13",
    "@nestjs/core": "7.6.13",
    "@nestjs/platform-express": "7.6.13",
    "@nestjs/testing": "7.6.13",
    "@types/ioredis": "4.19.4",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.31",
    "@types/supertest": "2.0.10",
    "ioredis": "4.22.0",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.6.3",
    "ts-jest": "26.5.1",
    "tslint": "6.1.3",
    "typescript": "4.1.5"
  },
  "peerDependencies": {
    "ioredis": "4.22.0",
    "crypto": "1.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
